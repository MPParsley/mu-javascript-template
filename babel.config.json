{
  "presets": [
    ["@babel/preset-env",
      {
        "targets": {
          "node": 14
        },
        "modules": false
      }
    ],
    ["@babel/preset-typescript"
    ]
    
  ],
  "sourceMaps": true,
  "plugins": [
    ["@babel/plugin-proposal-decorators", { legacy: true }],
    ["@babel/plugin-proposal-class-properties"],
    ["module-resolver", {
      "alias": {
        "mu": "/usr/src/output/helpers/mu/index.js"
      }
    }]
  ],
  "ignore": [
    "./node_modules",
    "./intermediate-transpilation/node_modules"
  ]
}
